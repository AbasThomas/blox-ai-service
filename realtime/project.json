{
  "name": "realtime",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "realtime/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/realtime",
        "main": "realtime/src/main.ts",
        "tsConfig": "realtime/tsconfig.app.json",
        "assets": ["realtime/src/assets"]
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "realtime:build"
      }
    },
    "test": {
      "options": {
        "passWithNoTests": true
      }
    }
  }
}

